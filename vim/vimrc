set nocompatible
filetype off
set rtp+=~/.vim/bundle/Vundle.vim
call vundle#begin()
Plugin 'VundleVim/Vundle.vim'
call vundle#end()
filetype plugin indent on
set backspace=eol,start,indent
set encoding=utf8
set ffs=unix,dos,mac
set hlsearch
set incsearch
set lazyredraw
set laststatus=2
set magic
set mat=2
set mouse=r
set nobackup
set noerrorbells
set noswapfile
set novisualbell
set nowritebackup
set number
set showmatch
